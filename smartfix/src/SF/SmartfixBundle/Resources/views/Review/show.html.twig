{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Review</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Assid</th>
                <td>{{ entity.assId }}</td>
            </tr>
            <tr>
                <th>Clientid</th>
                <td>{{ entity.clientId }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ entity.rating }}</td>
            </tr>
            <tr>
                <th>Ontime</th>
                <td>{{ entity.onTime }}</td>
            </tr>
            <tr>
                <th>Profix</th>
                <td>{{ entity.proFix }}</td>
            </tr>
            <tr>
                <th>Countesy</th>
                <td>{{ entity.countesy }}</td>
            </tr>
            <tr>
                <th>Clarityorexp</th>
                <td>{{ entity.clarityOrExp }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ entity.comment }}</td>
            </tr>
            <tr>
                <th>Datecreate</th>
                <td>{{ entity.dateCreate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('review') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('review_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
